{
	"schemaVersion": 1,
	"id": "autocut",
	"version": "${version}",
	"name": "Autocut",
	"description": "Cut your recordings based on in-game events.",
	"authors": [
		"skycatminepokie"
	],
	"contributors": [
		"SuperiorTabby"
	],
	"contact": {
		"sources": "https://github.com/skycatminepokie/autocut"
	},
	"license": "MIT",
	"icon": "assets/autocut/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.skycatdev.autocut.Autocut"
		],
		"client": [
			"com.skycatdev.autocut.AutocutClient"
		],
		"fabric-datagen": [
			"com.skycatdev.autocut.datagen.AutocutDataGenerator"
		],
		"modmenu": [
			"com.skycatdev.autocut.ModMenuCompat"
		]
	},
	"mixins": [
		"autocut.mixins.json",
		{
			"config": "autocut.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "1.21.1",
		"java": ">=21",
		"fabric-api": ">=0.104.0+1.21.1",
		"yet_another_config_lib_v3": ">=3.5.0+1.21-fabric"
	},
	"recommends": {
		"modmenu": ">=11.0.2"
	},
	"custom":{
		"mc-publish": {
			"dependencies": [
				"fabric-api(required)",
				"yet_another_config_lib_v3(required){modrinth:yacl}",
				"modmenu(recommended)"
			]
		}
	}
}