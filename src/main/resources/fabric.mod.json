{
	"schemaVersion": 1,
	"id": "autocut",
	"version": "${version}",
	"name": "Autocut",
	"description": "Cut your recordings based on in-game events.",
	"authors": [
		"skycatminepokie"
	],
	"contact": {
		"sources": "https://github.com/skycatminepokie/autocut"
	},
	"license": "MIT",
	"icon": "assets/autocut/icon.png",
